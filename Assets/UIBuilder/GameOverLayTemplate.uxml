            <ui:VisualElement name="CONTENT-IMAGE" style="max-width: 80%; min-height: 400px; width: 1093px; background-image: resource(&apos;Mapbox/PrefabScreens/WorldScaleAR&apos;); visibility: visible; -unity-background-scale-mode: scale-to-fit; -unity-font: url(&apos;project://database/Assets/Fonts/Fira_Sans/FiraSans-Medium.ttf?fileID=12800000&amp;guid=c8cad423f7ade48afb118eaa04dee0dd&amp;type=3#FiraSans-Medium&apos;);" />
