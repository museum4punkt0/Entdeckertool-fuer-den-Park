<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UIBuilder/styles/GameOverlay.uss?fileID=7433441132597879392&amp;guid=57feafdb8c9394024a77a47dbe1ff66d&amp;type=3#GameOverlay" />
    <ui:VisualElement name="GameOverlay" usage-hints="None" focusable="false" class="gameoverlay-container BasicButton" style="position: absolute; left: 0; top: -100%; right: 0; bottom: auto; background-color: rgb(16, 159, 148); justify-content: space-between; align-items: center; padding-top: 40%; padding-left: 10%; padding-right: 10%; padding-bottom: 10%; border-right-width: 0; translate: 0 0; max-height: 100%; margin-top: -20%; height: 100%;">
